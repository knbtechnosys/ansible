server {
    listen 80;
    server_name test1.playerzpot.com www.test1.playerzpot.com;

    root /var/www/code;
    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }

    error_page 500 502 503 504 /50x.html;
    location = /50x.html {
        root /usr/share/nginx/html;
    }
}
